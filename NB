void Menu( void )
{
  pritnf("0 - Exit\n"
         "1 - Add pupil\n"
         "2 - Delete by number\n"
         "3 - Output all pupils\n"
         "4 - Sort datebase\n"
         "5 - Flip database\n"
         "6 - Save database\n"
         "7 - load database\n");
  switch(getch()):
  {
  case '1':
  case '2':
  case '3':
  case '4':
  case '5':
  case '6':
  case '7':
  }
}

int main( void )
{
  Menu();
  return 0;
}
